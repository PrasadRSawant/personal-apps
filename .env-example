# .env content
# -----------------------------------------------

# PostgreSQL Credentials
POSTGRES_USER=username
POSTGRES_PASSWORD=password
POSTGRES_HOST=hostname
POSTGRES_PORT=5432
POSTGRES_DB=base_name

# Full URL for SQLAlchemy
# Note the f-string style placeholders here; we'll construct this in Python later.
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}

# .env file content
# ...
GOOGLE_CLIENT_ID=sample_google_client_id
GOOGLE_CLIENT_SECRET=sample_google_client_secret
REDIRECT_URI=http://localhost:8000/auth/google/callback

CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

REDIS_URL=redis://localhost:6379/0